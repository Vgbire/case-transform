{
  "name": "letter-transform",
  "publisher": "Vgbire",
  "displayName": "letTrans",
  "description": "Letter case transformation",
  "repository": "https://github.com/Vgbire/letter-transform",
  "icon": "https://github.com/Vgbire/letter-transform/blob/main/images/icon.png",
  "homepage": "https://github.com/Vgbire/letter-transform/blob/main/README.md",
  "keywords": [
    "transform",
    "upperCase",
    "lowerCase",
    "camel"
  ],
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:case-transform.upper"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "case-transform.upper",
        "title": "转成大写"
      },
      {
        "command": "case-transform.lower",
        "title": "转成小写"
      },
      {
        "command": "case-transform.camel",
        "title": "驼峰命名"
      },
      {
        "command": "case-transform.underline",
        "title": "下划线命名"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "case-transform.upper",
          "group": "navigation"
        },
        {
          "when": "editorHasSelection",
          "command": "case-transform.lower",
          "group": "navigation"
        },
        {
          "when": "editorHasSelection",
          "command": "case-transform.camel",
          "group": "navigation"
        },
        {
          "when": "editorHasSelection",
          "command": "case-transform.underline",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.59.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
