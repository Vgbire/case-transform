{
  "name": "letter-transform",
  "publisher": "Vgbire",
  "displayName": "letter-transform",
  "description": "Conversion for multi cursor string case, hump naming and underline naming",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vgbire/letter-transform"
  },
  "icon": "images/icon.png",
  "homepage": "https://github.com/Vgbire/letter-transform/blob/main/README.md",
  "keywords": [
    "transform",
    "upperCase",
    "lowerCase",
    "camel",
    "大写",
    "小写",
    "驼峰",
    "下划线",
    "转换"
  ],
  "version": "1.1.0",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Formatters"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "case-transform.camel",
        "title": "驼峰(CamelCased)"
      },
      {
        "command": "case-transform.underline",
        "title": "下划线(UnderScoreCase)"
      },
      {
        "command": "case-transform.kebab",
        "title": "短横线(KebabCase)"
      },
      {
        "command": "case-transform.upper",
        "title": "大写(Upper)"
      },
      {
        "command": "case-transform.lower",
        "title": "小写(Lower)"
      },
      {
        "command": "case-transform.pascal",
        "title": "帕斯卡(Pascal)"
      }
    ],
    "keybindings": [
      {
        "command": "case-transform.camel",
        "key": "ctrl+1",
        "mac": "cmd+1",
        "when": "editorHasSelection"
      },
      {
        "command": "case-transform.underline",
        "key": "ctrl+2",
        "mac": "cmd+2",
        "when": "editorHasSelection"
      },
      {
        "command": "case-transform.kebab",
        "key": "ctrl+3",
        "mac": "cmd+3",
        "when": "editorHasSelection"
      },
      {
        "command": "case-transform.upper",
        "key": "ctrl+4",
        "mac": "cmd+4",
        "when": "editorHasSelection"
      },
      {
        "command": "case-transform.lower",
        "key": "ctrl+5",
        "mac": "cmd+5",
        "when": "editorHasSelection"
      },
      {
        "command": "case-transform.pascal",
        "key": "ctrl+6",
        "mac": "cmd+6",
        "when": "editorHasSelection"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "case-transform.camel",
          "group": "navigation@5"
        },
        {
          "when": "editorHasSelection",
          "command": "case-transform.underline",
          "group": "navigation@6"
        },
        {
          "when": "editorHasSelection",
          "command": "case-transform.kebab",
          "group": "navigation@7"
        },
        {
          "when": "editorHasSelection",
          "command": "case-transform.upper",
          "group": "navigation@8"
        },
        {
          "when": "editorHasSelection",
          "command": "case-transform.lower",
          "group": "navigation@9"
        },
        {
          "when": "editorHasSelection",
          "command": "case-transform.pascal",
          "group": "navigation@10"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.59.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
